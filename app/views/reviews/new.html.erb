<%= simple_form_for [ @space, @review ] do |f| %>

  <%= f.input :rating, collection: (1..5).to_a %>
  <%= f.input :content %>

  <%= f.submit "Submit review", class: 'btn btn-primary' %>

  <%= link_to space_path(@space[:id]) do %>
    <%= f.button :button, "Cancel", type: "reset", class: " btn btn-outline-secondary" %>
  <% end %>

<% end %>
